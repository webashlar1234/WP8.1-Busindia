                    </ListView>
                </Grid>
            </Grid>
            <Grid x:Name="BusTypeGrid"  VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Visibility="Collapsed" >
                <Grid Grid.Row="0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Visibility="Visible" Background="Gray" Opacity="0.4">
                </Grid>
                <Grid Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Center" Background="White"   Width="400">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>

                    <RadioButton x:ConnectionId='2' x:Name="rdbFilterBusTypeALL" Background="White"  Margin="10,10,0,0"  FontSize="20"                                     Style="{StaticResource RadioButtonStyle3}"  BorderBrush="#8B8B8B"  GroupName="Bus" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="LightGray"  />
                    <TextBlock x:Name="txtBFilterBusTypeALL"  Text="ALL"  Foreground="Black" Margin="50,10,0,0" VerticalAlignment="Center" FontSize="20"  Grid.Column="0" Grid.Row="0"></TextBlock>
                    <ListView  Grid.Column="0" Grid.Row="2" x:Name="ListMenuItemsFilterBusType" >
                        <ListView.ItemTemplate>
                            <DataTemplate>
                                <StackPanel Orientation="Horizontal">
                                    <RadioButton x:ConnectionId='3' x:Name="rdbFilterBusType" Content="{Binding}" Foreground="White" Background="White"  Margin="10,0,0,0"  FontSize="20"                                  Style="{StaticResource RadioButtonStyle3}"  BorderBrush="#8B8B8B"  GroupName="Bus" HorizontalAlignment="Left" VerticalAlignment="Center"   >
                                        <RadioButton.RenderTransform>
                                            <CompositeTransform ScaleX="1"/>
                                        </RadioButton.RenderTransform>
                                    </RadioButton>
                                    <TextBlock   Foreground="Black" Text="{Binding}" Margin="10,0,0,0" VerticalAlignment="Center" FontSize="20" ></TextBlock>
                                </StackPanel>
                            </DataTemplate>
                        </ListView.ItemTemplate>
                    </ListView>
                </Grid>
            </Grid>
            <Grid x:Name="LoaderPop"  VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Visibility="Collapsed">
                <Grid Grid.Row="0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Visibility="Visible" Background="Gray" Opacity="0.2">
                </Grid>
                <Grid Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Center" Background="White"  Margin="50,0,50,0" Width="350" >
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <ProgressRing x:Name="progress1" Grid.Row="0" Height="45" Width="15" VerticalAlignment="Center" HorizontalAlignment="Center" IsActive="True" Background="White" Style="{StaticResource ProgressRingStyle1}" Margin="96,10,96,10"/>
                    <Grid Background="Gray" Grid.Row="1" Height="1"></Grid>
                    <TextBlock FontSize="15" Grid.Row="2" Margin="0,15,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="Black" Text="Searching Buses for "></TextBlock>
                    <StackPanel Orientation="Horizontal" Grid.Row="3" HorizontalAlignment="Center" Margin="0,15,0,15">
                        <TextBlock x:Name="lblFromPlace"  FontSize="18" Foreground="Black" ></TextBlock>
                        <TextBlock  Text="-" FontSize="18" Foreground="Black" Margin="0,0,5,0"></TextBlock>
                        <TextBlock x:Name="lblToPlace"  FontSize="18" Foreground="Black" Margin="0,0,5,0"></TextBlock>
                        <TextBlock Text="on" FontSize="18" Foreground="Black" Margin="0,0,5,0" ></TextBlock>
                        <TextBlock x:Name="lblJrnyDate"  FontSize="18" Foreground="Black" Margin="0,0,5,0"></TextBlock>
                    </StackPanel>
                </Grid>
            </Grid>
            <Grid x:Name="Error" Grid.RowSpan="3" VerticalAlignment="Bottom" Margin="0,0,0,20" HorizontalAlignment="Stretch" Visibility="Collapsed">
                <Grid Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Center" Background="Transparent"  >
                    <Border CornerRadius="5" BorderBrush="Gray" Background="Gray" Opacity=".7">
                        <TextBlock FontSize="12" FontWeight="Bold" Grid.Row="2" Margin="30,10,30,10" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White" Text="There is no service available"></TextBlock>
                    </Border>
                </Grid>
            </Grid>
            <Grid x:Name="ErrorAdvanceBooking" Grid.RowSpan="3" VerticalAlignment="Bottom" Margin="0,0,0,20" HorizontalAlignment="Stretch" Visibility="Collapsed">
                <Grid Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Center" Background="Transparent"  >
                    <Border CornerRadius="5" BorderBrush="Gray" Background="Gray" Opacity=".7">
                        <TextBlock FontSize="12" FontWeight="Bold" Grid.Row="2" Margin="20,10,20,10" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White" Text="Sorry you can book a ticket only in advanced of 15 days"></TextBlock>
                    </Border>
                </Grid>
            </Grid>
        </Grid>
        <Grid x:Name="gridErrorOccured" HorizontalAlignment="Center" VerticalAlignment="Center"></Grid>
    </Grid>
</Page>

